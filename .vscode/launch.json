        {
            // "name": "Python: fbref",
            "name": "Python: caring",
            "type": "python",
            "request": "launch",
            // ${workspaceFolder} is the root of the opened directory (working directory opend by VS Code)
            "program": "${workspaceFolder}/.venv/bin/scrapy",
            "args": [
                "crawl",
                "caring_1",
            ],
            // need to run spider in the top directory of scrapy project
